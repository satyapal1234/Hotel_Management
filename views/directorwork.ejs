<script>

$(document).ready(function(){
   $("#myInput2").on("keyup", function() {
    var value = $(this).val().toLowerCase();
    $("#myTable2 tr").filter(function() {
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
  });
});

  
  document.getElementById("showbookedRoomInCity").style.display="none";
  function showBookedRoomCity()
  {
    document.getElementById("showbookedRoomInCity").style.display="block";
  }
</script>
